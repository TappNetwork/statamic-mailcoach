{
    "name": "spatie/statamic-mailcoach",
    "type": "statamic-addon",
    "autoload": {
        "psr-4": {
            "Spatie\\StatamicMailcoach\\": "src"
        }
    },
    "require": {
        "spatie/laravel-mailcoach": "dev-main"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/TappNetwork/laravel-mailcoach"
        }
    ],
    "extra": {
        "statamic": {
            "name": "Statamic Mailcoach",
            "description": "Display a Mailcoach summary inside Statamic"
        },
        "laravel": {
            "providers": [
                "Spatie\\StatamicMailcoach\\StatamicMailcoachServiceProvider"
            ]
        }
    }
}
